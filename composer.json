{
    "name": "payum/payline",
    "type": "project",
    "description": "The Payum extension. Rapid extensino development",
    "keywords": ["payment"],
    "homepage": "http://www.khedhiri.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Khedhiri",
            "email": "claude@khedhiri.com"
        }
    ],
    "require": {
        "payum/core": "^1.1.3"
    },
    "require-dev": {
        "payum/core": "~1.0@dev",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "5.3.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Payum\\Payline\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Payum\\Payline\\Test\\": "tests/" }
    },
    "target-dir": "Payum/Payline",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
